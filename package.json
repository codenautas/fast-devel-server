{
  "name": "fast-devel-server",
  "description": "Fast developer server for displaying markdown and others",
  "version": "0.2.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/fast-devel-server",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  }],
  "main": "server/server.js",
  "files": [
    "img",
    "server"
  ],
  "dependencies": {
    "express": "~4.15.3",
    "fs-extra": "~3.0.1",
    "fs-promise": "~2.0.3",
    "highlight.js": "~9.11.0",
    "pug": "~2.0.0-rc.1",
    "lodash": "~4.17.4",
    "moment": "~2.18.1",
    "multilang": "~1.0.19",
    "numeral": "~2.0.6",
    "pygmentize-bundled": "~2.3.0",
    "remarkable": "~1.7.1",
    "serve-index": "~1.8.0",
    "stylus": "~0.54.5",
    "to-binary": "~0.0.1",

    "ajax-best-promise": "~0.3.3",
    "auto-deploy": "~0.0.8",
    "best-promise": "~0.2.4",
    "dir-info": "~1.0.10",
    "exec-to-html": "~1.0.5",
    "extension-serve-static": "~0.1.6",
    "js-to-html": "~0.9.2",
    "kill-9": "~0.4.2",
    "mini-tools": "~0.3.16",
    "qa-control": "~0.4.10"
  },
  "x": {
    "brucedown": "^1.1.1",
    "markdown": "^0.5.0",
    "markdown-it": "^4.2.0"
  },
  "devDependencies": {
    "after": "~0.8.2",
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.5",
    "mocha": "~3.4.2",
    "supertest": "~3.0.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-ci z": "istanbul cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node server/server.js",
    "auto-deploy": ""
  },
  "qa-control": {
    "package-version": "0.3.0",
    "run-in": "server",
    "stability": "extending",
    "type": "app"
  }
}
