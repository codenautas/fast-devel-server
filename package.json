{
  "name": "fast-devel-server",
  "description": "Fast developer server for displaying markdown and others",
  "version": "0.1.3",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/fast-devel-server",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  }],
  "main": "server/server.js",
  "dependencies": {
    "express": "~4.14.0",
    "fs-extra": "~0.30.0",
    "fs-promise": "~0.5.0",
    "highlight.js": "~9.7.0",
    "pug": "~2.0.0-beta6",
    "lodash": "~4.16.2",
    "moment": "~2.15.1",
    "multilang": "~1.0.14",
    "numeral": "~1.5.3",
    "pygmentize-bundled": "~2.3.0",
    "remarkable": "~1.7.0",
    "serve-index": "~1.8.0",
    "stylus": "~0.54.5",
    "to-binary": "~0.0.1",

    "ajax-best-promise": "~0.2.0",
    "auto-deploy": "~0.0.8",
    "best-promise": "~0.2.4",
    "dir-info": "~1.0.10",
    "exec-to-html": "~1.0.2",
    "extension-serve-static": "~0.1.6",
    "js-to-html": "~0.9.2",
    "kill-9": "~0.4.2",
    "mini-tools": "~0.3.3",
    "qa-control": "~0.4.1"
  },
  "x": {
    "brucedown": "^1.1.1",
    "markdown": "^0.5.0",
    "markdown-it": "^4.2.0"
  },
  "devDependencies": {
    "after": "~0.8.2",
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.5",
    "mocha": "~3.1.0",
    "supertest": "~2.0.0"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-ci z": "istanbul cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node server/server.js",
    "auto-deploy": ""
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true
  },
  "qa-control": {
    "package-version": "0.0.1",
    "run-in": "server",
    "stability": "extending",
    "type": "app"
  }
}
