{
  "name": "fast-devel-server",
  "description": "Fast developer server for displaying markdown and others",
  "version": "0.0.9",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/fast-devel-server",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  }],
  "main": "server/server.js",
  "dependencies": {
    "express": "^4.13.3",
    "fs-extra": "^0.24.0",
    "fs-promise": "~0.3.1",
    "highlight.js": "^8.7.0",
    "jade": "^1.11.0",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "moment": "^2.10.6",
    "multilang": "^1.0.4",
    "numeral": "~1.5.3",
    "pygmentize-bundled": "^2.3.0",
    "serve-index": "1.7.2",
    "to-binary": "~0.0.1",

    "ajax-best-promise": "~0.0.7",
    "auto-deploy": "~0.0.8",
    "best-promise": "~0.2.3",
    "dir-info": "~0.1.3",
    "exec-to-html": "^0.0.7",
    "extension-serve": "^0.0.4",
    "js-to-html": "~0.4.1",
    "kill-9": "~0.2.3",
    "mini-tools": "~0.1.2",
    "qa-control": "^0.0.9"
  },
  "x": {
    "brucedown": "^1.1.1",
    "markdown": "^0.5.0",
    "markdown-it": "^4.2.0"
  },
  "devDependencies": {
    "istanbul": "~0.3.19",
    "mocha": "~2.3.0",
    "after": "~0.8.1",
    "supertest": "~1.1.0",
    "expect.js": "^0.3.1"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-ci z": "istanbul cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node node_modules/auto-deploy/auto-deploy-runner.js -- server/server.js",
    "auto-deploy": ""
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true
  },
  "qa-control": {
    "package-version": "0.0.1",
    "run-in": "server",
    "stability": "designing",
    "type": "app"
  }
}
